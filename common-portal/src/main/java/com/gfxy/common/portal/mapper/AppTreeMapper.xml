    <select id="getAdminAppById" parameterType="java.lang.String" resultMap="BaseResultMap">
        SELECT DISTINCT A.*
        FROM APP_TREE A
        WHERE A.ID = #{menuId}
    </select>

  <select id="getUserAppById" parameterType="java.lang.String" resultMap="BaseResultMap">
        SELECT DISTINCT A.*
        FROM APP_TREE A, ROLE_RESOURCES B
        WHERE B.TYPE = 0
              AND A.ID = B.RESOURCE_ID
              AND B.ROLE_ID IN (SELECT C.ROLE_ID
                                FROM ROLE_RESOURCES C, USER D
                                WHERE C.RESOURCE_ID = D.USER_ID
                                      AND C.TYPE = 5
                                      AND D.LOGIN_NAME = #{userName})
              AND A.ID = #{menuId}
    </select>
