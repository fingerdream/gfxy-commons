  <select id="getAppsByAdmin" parameterType="java.lang.String" resultMap="BaseResultMap">
        SELECT DISTINCT
            A.ID,
            A.PARENT_ID,
            A.LABEL,
            A.RELATING_ID,
            CASE
            WHEN INSTR(A.URL, 'http') > 0
                THEN A.URL
            WHEN A.URL IS NOT NULL
                THEN
                    concat('authMenu/getMenuAuth.do?menuId=' , A.ID)
            ELSE
                A.Url
            END                 URL,
            IFNULL(A.ORDER_NUM, 0) ORDER_NUM,
            A.IS_LEAF,
            A.ICON_CLS,
            A.OPEN_TYPE
        FROM APP_TREE A
        WHERE A.PARENT_ID NOT IN ('-2')
        AND FIND_IN_SET(PARENT_ID,getChildrenMenu(#{parentId}))
        ORDER BY PARENT_ID ASC, ORDER_NUM ASC
    </select>
